{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "packageRules": [
    {
      "packagePatterns": ["*"],
      "excludePackagePatterns": [
        "typescript",
        "vitest",
        "jsdom",
        "prettier",
        "vite-tsconfig-paths",
        "react",
        "@types/react",
        "@total-typescript/exercise-cli",
        "zod"
      ],
      "enabled": false
    }
  ]
}
